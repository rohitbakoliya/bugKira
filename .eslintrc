{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "prettier",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier/react"
  ],
  "plugins": ["react", "prettier", "react-hooks", "jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {},
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
